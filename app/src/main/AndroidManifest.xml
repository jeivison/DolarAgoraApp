<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dolarhoje">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DolarHoje">
        <activity android:name=".activity.TelaDolar" />
        <activity android:name=".activity.TelaLogin" />
        <activity android:name=".activity.TelaCadastro" />
        <activity
            android:name=".activity.telaCadastro"
            android:label="@string/title_activity_tela_cadastro"
            android:theme="@style/Theme.DolarHoje.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.Intro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>